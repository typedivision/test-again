#!/usr/bin/env sh

test_exit_pass () {
  exit
}

test_return_pass () {
  return
}

test_expect_status_true_pass () {
  t_expect_status true 0
}

test_expect_status_false_pass () {
  t_expect_status false 1
}

test_expect_output_pass () {
  t_expect_output 'echo "hello world"' "hello world"
}

test_expect_output_multiline_pass () {
  t_expect_output 'for i in hello world; do echo $i; done' "\
hello
world"
}

test_expect_value_pass () {
  t_expect_value 1 1
}

test_expect_value_char_pass () {
  local a="x"
  t_expect_value '$a' x
}

test_expect_value_number_pass () {
  local a=0
  t_expect_value '$a' 0
}

test_multiple_expects_pass () {
  t_expect_status true 0
  t_expect_output "echo 0" "0"
  t_expect_value  0 0
}

. test/helper
. tea.sh
